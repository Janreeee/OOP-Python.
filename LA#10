class Vehicle():
    def __init__(self,brand,model,fuel_types):
        self.brand = brand
        self.model = model
        self.fuel_types = fuel_types
        
    def describeVehicle(self):
        print(f'{self.brand},{self.model},{self.fuel_types}')
        
class Car():
    pass
    
class Motorcycle():
    pass

bmw = Vehicle("BMW","XM SUV","Diesel")
bmw.describeVehicle()
