LA#30
import tkinker as tk

windows = tk.Tk()
windows.title("")
windows.geometry("800x600")
windows.configure(bg="green")

name = tk.Entry(windows)
name.grid(row=1, column=0)

def printmo():
    print(name.get())
button = tk.Button(windows, text="Daniel", command=printmo)
button.grid(row=1, column=1, pady=20, padx=20)

windows.mainloop()
