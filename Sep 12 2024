class phone:
    def __init__(self,brand):
        self.brand = brand
    
    def phone_info(self):
        pass

p1 = phone("Samsung")
print(p1.phone_info)  

class student:
    pass
daniel = student()
print(daniel) 

class phone:
    def __init__(self,brand):
        self.brand = brand # Attribute / Property
p1 = phone("Samsung")

print(f"Original Value: {p1.brand}")

p1.brand = "Xiaomi" #Update the attribute
    
print(f"Updated Value: {p1.brand}") 

class course:
    def __init__(self,name):
        self.name = name
it = course("Information Technology")
cs = course("Computer Science")
print(cs.name)
cs.name = "BSIT" #Update Value
print(cs.name) 

class food:
    def __init__(self, name):
        self.name = name
burger = food("Angels")
del burger.name
print(burger.name)

