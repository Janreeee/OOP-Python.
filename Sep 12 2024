LA#7
class Car:
    def __init__(self,brand,color):
        self.brand = brand
        self.color = color

vehicle = Car("BMW", "White")
cars = Car("Honda", "Black")
print(vehicle.brand, vehicle.color)
vehicle.color = "Red"
print(vehicle.brand, vehicle.color)
print(cars.brand, cars.color)

LA#8
class Book:
    def __init__(self ,title ,author):
        self.title = title
        self.author = author
    
Title = Book("English", "James")
Title1 = Book("Science", "Daniel")
print(f'{Title.title}, {Title.author}')
del Title1.author
